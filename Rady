#include <iostream>
#include <string> 

using namespace std;

int main()
{
	for (int i1 = 1; i1 <= 9; i1++) {
		for (int i2 = 0; i2 <= 9; i2++) {
			if (i1 != i2) {
				for (int i3 = 0; i3 <= 9; i3++) {
					if (i1 != i3 && i2 != i3) {
						for (int i4 = 0; i4 <= 9; i4++) {
							if (i1 != i4 && i2 != i4 && i3 != i4) {
								cout << to_string(i1) << to_string(i2) << to_string(i3) << to_string(i4) << endl;
								cout << "-" << to_string(i1) << to_string(i2) << to_string(i3) << to_string(i4) << endl;
							}
						}
					}
				}
			}
		}
	}
}
