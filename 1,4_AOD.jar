public class ginfin {
    public static int f(int n) {
        if (n > 2) {
            return f(n-1)+g(n-2);
        }else {

            return n;
        }
    }
    public static int g(int n) {

        if (n > 2) {
           return g(n-1)-f(n-2);
        }else{
            return n+1;
        }
    }

    public static void main(String[] args) {
       System.out.println(f(6) );
    }
}
